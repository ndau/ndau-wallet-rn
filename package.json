    "test-ci": "jest --ci --runInBand",
    "testnew": "jest -o",
    "xcode-open": "open ios/ndauwalletrn.xcodeproj",
    "android-install": "cd android && ./gradlew installDebug",
    "clear-cache": "watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf /tmp/haste-map-react-native-packager-*",
